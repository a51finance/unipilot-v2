{
  "name": "unipilot-v2",
  "description": "Unipilot - Universal automated liquidity optimization",
  "version": "1.0.0",
  "author": {
    "name": "VoirSudio"
  },
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@ethersproject/abi": "^5.2.0",
    "@ethersproject/abstract-signer": "^5.2.0",
    "@ethersproject/bignumber": "^5.2.0",
    "@ethersproject/bytes": "^5.2.0",
    "@ethersproject/contracts": "^5.2.0",
    "@ethersproject/providers": "^5.2.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.2.0",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.6.4",
    "hardhat-gas-reporter": "^1.0.6",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "lodash": "^4.17.21",
    "mocha": "^9.1.3",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.3.0",
    "typechain": "^5.2.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "/contracts"
  ],
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf ./artifacts ./cache ./coverage ./src/types ./coverage.json",
    "commit": "git-cz",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "yarn typechain && hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
    "deploy": "hardhat run scripts/deploy.ts",
    "deploy-vaultOnly": "hardhat deploy-unipilot --network",
    "deploy-vault": "hardhat deploy-vault --network",
    "deploy-strategy": "hardhat deploy-strategy --network",
    "deploy-factory-active": "hardhat deploy-unipilotFactory-active --network",
    "deploy-factory-passive": "hardhat deploy-unipilotFactory-passive --network",
    "deploy-migrator": "hardhat deploy-migrator --network",
    "verify-vault-active": "hardhat verify-active-vault --network",
    "verify-vault-passive": "hardhat verify-passive-vault --network",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "verify": "hardhat verify --network",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "test": "hardhat test",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain"
  },
  "dependencies": {
    "@nomiclabs/hardhat-etherscan": "^3.1.6",
    "@openzeppelin/contracts": "3.4.1-solc-0.7-2",
    "@openzeppelin/contracts-upgradeable": "3.3.0",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/v3-core": "^1.0.0",
    "@uniswap/v3-periphery": "^1.3.0",
    "@uniswap/v3-sdk": "^3.8.1",
    "hardhat-contract-sizer": "^2.3.0",
    "mocha-chai-jest-snapshot": "^1.1.3",
    "uniswap-v3-deploy-plugin": "^0.1.0"
  }
}
